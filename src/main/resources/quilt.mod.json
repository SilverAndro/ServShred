{
  "schema_version": 1,
  "quilt_loader": {
    "group": "io.github.silverandro",
    "id": "servshred",
    "version": "${version}",
    "metadata": {
      "name": "ServShred",
      "description": "A server side veinminer.",
      "license": "MPL-2.0",
      "contributors": {
        "Silver": "Owner"
      },
      "contact": {
        "issues": "https://github.com/SilverAndro/ServShred/issues",
        "sources": "https://github.com/SilverAndro/ServShred"
      },
      "icon": "assets/example_mod/icon.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": {
        "adapter": "kotlin",
        "value": "io.github.silverandro.servshred.ServShredMain"
      }
    },
    "depends": [
      {
        "id": "quilt_loader",
        "versions": ">=0.17.0-"
      },
      {
        "id": "quilted_fabric_api",
        "versions": ">=4.0.0-"
      },
      {
        "id": "qkl",
        "versions": ">=1.1.0"
      },
      {
        "id": "minecraft",
        "versions": ">=1.19.3"
      }
    ]
  }
}